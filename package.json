{
  "name": "project-symbol-explorer-cheonglol",
  "displayName": "Project Symbol Explorer",
  "description": "A VS Code extension that scans the workspace for all types, classes, and symbols (TypeScript, extensible to other languages) and renders a visual display of all symbols and objects using VS Code APIs.",
  "version": "0.0.1",
  "publisher": "Lester Cheong",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "project-symbol-explorer-cheonglol.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "project-symbol-explorer-cheonglol.toggleSymbolExplorer",
        "title": "Toggle Project Symbol Explorer"
      }
    ]
  },
  "scripts": {
    "compile": "vsce package",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run package && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
